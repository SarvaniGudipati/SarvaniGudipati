import java.util.*;
class  Main
{
public static void main(String args[])
{
ArrayList<Integer>a1=new ArrayList<Integer>();
ArrayList<Integer>a2=new ArrayList<Integer>();
a1.add(10);
a1.add(20);
a1.add(30);
a1.add(40);
a2.add(100);
a2.add(200);
a2.add(300);
a2.add(400);
a1.addAll(a2);
System.out.print(a1);
}
}
