import java.util.*;
public class Palindrome
{
    public static void main(String args[])
    {
        String str,rev="";
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a string:");
        str=sc.nextLine();
        str=str.toLowerCase();
        int n=str.length();
        for(int i=n-1;i>=0;i--)
        {
            rev=rev+str.charAt(i);
        }
        if(str==rev)
        {
            System.out.print("Palindrome");
        }
        else
        {
            System.out.print("Not Palindrome");
        }
    }
}
