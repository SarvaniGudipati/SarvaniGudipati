import java.util.*;
class prefixfact
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int q=sc.nextInt();
        int n=1000;
        int[] prefix= new int[n+1];
        prefix[0]=1;
        for(int i=1;i<n+1;i++)
        {
            prefix[i]=prefix[i-1]*i;
        }
        for(int j=0;j<q;j++)
        {
            int fact=sc.nextInt();
            System.out.println(prefix[fact]);
        }
        
    }
}
